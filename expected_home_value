import random

function for base cost randomized by zipcodede
def get_base_cost(zip_code):
    zip_code = int(zip_codison
    for state, ranges in zip_code_data.items():
        for entry in ranges:
            zip_range = entry["range"]
            base_cost = entry["base_cost"]
            if "-" in zip_range:
                start, end = map(int, zip_range.split("-"))
                if start <= zip_code <= end:
                    return base_cost
            else:
                if int(zip_range) == zip_code:
                    return base_cost
   return none if zip code not foundP c ee not found

